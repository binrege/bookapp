.rate {
    width: 100%;
    position: relative;
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
}

.rate__item {
    height: 60rpx;
    width: 56rpx;
    display: flex;
    align-items: center;
}

.icon-empty {
    background-position: 0 0rpx;
}

.icon-half {
    background-position: -153rpx 0rpx;
}

.icon-full {
    background-position: -203rpx 0rpx;
}

.icon-placeholder-empty {
    background-position: 0 0rpx;
}

.icon-placeholder-half {
    background-position: -51rpx 0rpx;
}

.icon-placeholder-full {
    background-position: -102rpx 0rpx;
}

.rate__item--icon {
    height: 46rpx;
    width: 46rpx;
    display: flex;
    flex-wrap: nowrap;
    align-items: center;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-size: 250rpx 46rpx;
}

.rate__item--icon>view {
    height: 100%;
    width: 50%;
}