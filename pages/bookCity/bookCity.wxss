.item {
    position: relative;
    text-align: center;
}

.tab {
    padding: 0 10rpx;
}

.item>view {
    text-align: center;
}

.line {
    position: absolute;
    width: 100%;
    height: 4rpx;
    background: red;
    bottom: -20rpx;
}

.fixed {
    position: fixed;
    top: 0;
    left: 0;
    height: 60rpx;
    width: 100%;
    background-color: white;
    z-index: 999;
}

.contant {
    position: relative;
    margin-top: 60rpx;
}